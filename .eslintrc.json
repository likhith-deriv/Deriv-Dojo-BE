{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "error",
        "no-eval": "error",
        "no-implied-eval": "error",
        "no-unused-vars": "warn",
        "no-loop-func": "error",
        "no-new-func": "error",
        "no-new": "error",
        "semi": [
            2,
            "always",
            {
                "omitLastInOneLineBlock": true
            }
        ]
    }
}
